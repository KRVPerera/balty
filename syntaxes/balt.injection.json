{
    "scopeName": "comment.annotation.injection",
    "injectionSelector": "L:comment",
    "patterns": [
      {
        "include": "#todo-keyword"
      },
      {
        "include": "#output-keyword"
      }
    ],
    "repository": {
      "todo-keyword": {
        "match": "TODO",
        "name": "keyword.todo"
      },
      "output-keyword": {
          "match": "output",
          "name": "keyword.output"
      }
    }
  }
